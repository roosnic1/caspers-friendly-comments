        "eslint": "^8.22.0",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "prettier": "^2.7.1",
        "prisma": "4.2.1",
        "ts-node": "10.9.1",
